{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "array-callback-return": "off",
    "consistent-return": "off",
    "camelcase": "off"
  }
}
